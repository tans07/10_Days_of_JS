function isPositive(a) {
    try{
        if(a > 0)
        {
            return "YES";
        }
       
        else if(a == 0)
        {
            return "Zero Error";
        }
        else
        {
            return "Negative Error";
        }
    }
    catch(err){
          console.log(err);
    }
    
}
